{
    "name": "ad blocker",
    "version": "1.0",
    "manifest_version": 2,
    "description": "This extension will block all ads",
    "permissions":[
        "activeTab",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "default_popup": "popup.html",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["background.js"]
        }
    ]
}